<!DOCTYPE html>
<html lang="en">
<head>
  {{> head}}
  <title>{{title}} | Silvie Tillard Fotografie</title>
</head>
<body>
  {{> site-header subpage=true}}
  <div class="columns">
    {{#each columns}}
    <div class="column">
      <div class="column-title">{{title}}</div>
      <div class="column-content">{{{content}}}</div>
      <div class="column-footer">{{{footer}}}</div>
    </div>
    {{/each}}
  </div>
</body>
</html>
